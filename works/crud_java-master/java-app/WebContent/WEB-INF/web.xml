<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.item.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>addItem</servlet-name>
    <servlet-class>com.item.AddItem</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>viewItem</servlet-name>
    <servlet-class>com.item.ViewItems</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>editItem</servlet-name>
    <servlet-class>com.item.EditItemPage</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>edit</servlet-name>
    <servlet-class>com.item.EditItem</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>deleteItem</servlet-name>
    <servlet-class>com.item.DeleteItem</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Menu</servlet-name>
    <servlet-class>com.item.Menu</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddItemPage</servlet-name>
    <servlet-class>com.item.AddItemPage</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Search</servlet-name>
    <servlet-class>com.item.Search</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addItem</servlet-name>
    <url-pattern>/addItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>viewItem</servlet-name>
    <url-pattern>/ViewItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>editItem</servlet-name>
    <url-pattern>/editItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deleteItem</servlet-name>
    <url-pattern>/deleteItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>edit</servlet-name>
    <url-pattern>/edit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Menu</servlet-name>
    <url-pattern>/Menu</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddItemPage</servlet-name>
    <url-pattern>/AddItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Search</servlet-name>
    <url-pattern>/Search</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/ViewItem</welcome-file>
  </welcome-file-list>
</web-app>